<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:f="urn:import:org.fedorahosted.flies.webtrans.client">

	<ui:with field='resources' type="org.fedorahosted.flies.webtrans.client.Resources"/>

	<ui:style>
	</ui:style>
	
	<g:LayoutPanel>
		<g:layer top='0px' height='20px'>
			<g:SimplePanel>
				<g:TextBox ui:field='filterTextBox'></g:TextBox>
			</g:SimplePanel>
		</g:layer>
		<g:layer top='20px' bottom='0px'>
			<g:ScrollPanel ui:field='documentScrollPanel'>
				<g:FlowPanel ui:field='documentList' />
			</g:ScrollPanel>
		</g:layer>
	</g:LayoutPanel>
	
</ui:UiBinder> 