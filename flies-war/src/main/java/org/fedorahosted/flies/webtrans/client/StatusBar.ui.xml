<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.complete {
			background: #D0EFE0;
		}
		
		.inProgress {
			background: #EFE9E0;
		}
		
		.unfinished {
			background: #EFDFE0;
		}
		
		.undefined {
			background: #EFEFE0;
		}
		
	</ui:style>
	<g:LayoutPanel ui:field='layoutPanel'>
		<g:layer left='0' width='0'>
			<g:FlowPanel ui:field='completePanel' styleName="{style.complete}"/>
		</g:layer>
		<g:layer left='0' width='0'>
			<g:FlowPanel ui:field='inProgressPanel' styleName="{style.inProgress}"/>
		</g:layer>
		<g:layer left='0' width='0'>
			<g:FlowPanel ui:field='unfinishedPanel' styleName="{style.unfinished}"/>
		</g:layer>
		<g:layer left='0' width='100%'>
			<g:FlowPanel ui:field='undefinedPanel' styleName="{style.undefined}"/>
		</g:layer>
	</g:LayoutPanel>
</ui:UiBinder> 