<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module>

      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>
      <inherits name="com.google.gwt.inject.Inject" />
      
      <inherits name='net.customware.gwt.dispatch.Dispatch' />
      <inherits name='net.customware.gwt.presenter.Presenter' />
      
      <inherits name='com.google.gwt.gen2.table.Table'/>
      <inherits name='com.google.gwt.gen2.complexpanel.ComplexPanel'/>
      
      <inherits name="com.weborient.codemirror.CodemirrorGwt"/>
       
      <inherits name="org.fedorahosted.flies.gwt.Common"/>
      
      <!--  inherit css based theme -->
      <inherits name='com.google.gwt.user.theme.standard.Standard'/>

      <!-- Specify the app entry point class.                         -->
      <entry-point class='org.fedorahosted.flies.webtrans.client.Application'/>

      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='Application.css' />

      <!-- Add gwt-log support, default level `OFF` - check for
           extended property 'log_level' to see if this is overridden -->
      <inherits name="com.allen_sauer.gwt.log.gwt-log-OFF" />

      <!-- Also compile Logger at `INFO` level -->
      <extend-property name="log_level" values="INFO" />
      <set-property name="log_level" value="INFO" />
      
      <!-- Turn off the floating logger - output will be shown in the hosted mode console -->
      <set-property name="log_DivLogger" value="DISABLED" />
    
      <source path="client" />

</module>
