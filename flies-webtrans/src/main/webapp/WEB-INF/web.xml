<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Flies WebTrans GWT Application</display-name>

	<servlet>
	  <servlet-name>EventService</servlet-name>
	  <servlet-class>
	    de.novanic.eventservice.service.EventServiceImpl
	  </servlet-class>
	</servlet>
	
	<servlet-mapping>
	  <servlet-name>EventService</servlet-name>
	  <url-pattern>/org.fedorahosted.flies.webtrans.Application/gwteventservice</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>
